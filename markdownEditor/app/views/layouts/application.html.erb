<!DOCTYPE html>
<html>
<head>
  <title>MarkdownEditor</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
        <%=link_to 'home', home_textEditor_path %>

    <%=link_to 'notes', notes_path %>
    <%=link_to 'trashcan', controller: trashcan_path %>
     
     <% if flash[:notice] %>
   		<p id="notice"><%= notice %></p>
   	 <% end %>

  </nav>

<%= yield %>

</body>
</html>
